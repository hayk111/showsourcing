{
  "name": "showsourcin-supplier-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:fr": "ng serve --configuration=fr --proxy-config proxy.config.json",
    "build": "ng build --prod --aot",
    "build:fr": "ng build --configuration=fr --prod",
    "translate": "ng xi18n --output-path locale",
    "translate:fr": "ng xi18n --output-path locale && xliffmerge --profile xliffmerge.json fr",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/showsourcing/stats.json",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "sync": "npm run build --prod && cd dist/showsourcing && aws s3 sync . s3://aws-website-showsourcing-web-dnz2m --delete",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@fortawesome/fontawesome": "^1.1.3",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.6",
    "@ng-select/ng-select": "^2.0.3",
    "apollo-angular": "^1.1.2",
    "apollo-angular-link-http": "^1.1.1",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-ws": "^1.0.8",
    "core-js": "^2.5.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "rxjs": "^6.2.1",
    "subscriptions-transport-ws": "^0.9.11",
    "uuid": "^3.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.0-beta.1",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@compodoc/compodoc": "^1.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/uuid": "^3.4.3",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-bundle-analyzer": "^2.9.2"
  }
}
