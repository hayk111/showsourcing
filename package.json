{
  "name": "showsourcin-supplier-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "sh ./scripts/start.sh",
    "serve": "ng serve",
    "serve:fr": "ng serve --configuration=fr",
    "serve:local": "ng serve --configuration=local",
    "build": "ng build --prod --aot",
    "build:dev": "ng build --configuration=dev --aot",
    "build:sta": "ng build --configuration=sta --aot",
    "build:prod": "npm run build",
    "translate": "ng xi18n --output-path locale",
    "translate:fr": "ng xi18n --output-path locale && xliffmerge --profile xliffmerge.json fr",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/showsourcing/stats.json",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "deploy:dev": "npm run build:dev && cd dist/showsourcing && aws s3 sync . s3://app-dev.showsourcing.com --delete && aws s3 cp s3://app-dev.showsourcing.com/index.html $URL/index.html --metadata-directive REPLACE --cache-control max-age=0",
    "deploy:sta": "npm run build:sta && cd dist/showsourcing && aws s3 sync . s3://app-sta.showsourcing.com --delete && aws s3 cp s3://app-sta.showsourcing.com/index.html s3://app-sta.showsourcing.com/index.html --metadata-directive REPLACE --cache-control max-age=0",
    "deploy:production": "npm run build && cd dist/showsourcing && aws s3 sync . s3://app.showsourcing.com --delete && aws s3 cp s3://app.showsourcing.com/index.html s3://app.showsourcing.com/index.html --metadata-directive REPLACE --cache-control max-age=0",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.0",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/pwa": "^0.13.1",
    "@angular/router": "^7.2.0",
    "@angular/service-worker": "7.2.4",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@ng-select/ng-select": "^2.14.0",
    "angulartics2": "^7.4.1",
    "apollo-angular": "^1.3.0",
    "apollo-angular-link-http": "^1.2.0",
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link-ws": "^1.0.10",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.0",
    "getstream": "^4.1.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "hubspot": "^2.1.1",
    "npm": "^6.4.1",
    "realm-graphql-client": "^1.3.0",
    "rxjs": "^6.4.0",
    "subscriptions-transport-ws": "^0.9.14",
    "uuid": "^3.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.1",
    "@angular/cli": "^7.1.0",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@compodoc/compodoc": "^1.1.7",
    "@types/file-saver": "^2.0.0",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.9.3",
    "@types/uuid": "^3.4.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.0",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "typescript": "^3.1.6",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}