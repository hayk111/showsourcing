{
  "name": "showsourcin-supplier-app",
  "version": "3.0.0-60",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "sh ./scripts/start.sh",
    "serve": "ng serve",
    "serve:supp": "ng serve supplier --port 4300",
    "serve:fr": "ng serve --configuration=fr",
    "serve:local": "ng serve --configuration=local",
    "build": "ng build --prod",
    "build:dev": "ng build",
    "build:sta": "ng build --configuration=sta",
    "build:prod": "npm run build",
    "build:supp": "ng build supplier --prod",
    "build:supp:dev": "ng build supplier",
    "build:supp:sta": "ng build supplier --configuration=sta",
    "version:build": "npm version prerelease -f",
    "version:patch": "npm version patch -f",
    "version:minor": "npm version minor -f",
    "translate": "ng xi18n --output-path locale",
    "translate:fr": "ng xi18n --output-path locale && xliffmerge --profile xliffmerge.json fr",
    "translate:zh-Hans": "ng xi18n --outputPath locale && xliffmerge --profile xliffmerge.json zh-Hans",
    "hmr": "ng serve --configuration hmr",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/showsourcing/stats.json",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "deploy:dev": "npm run build:dev && cd dist/showsourcing && aws s3 sync . s3://app-dev.showsourcing.com --delete && aws s3 cp s3://app-dev.showsourcing.com/index.html $URL/index.html --metadata-directive REPLACE --cache-control max-age=0",
    "deploy:sta": "npm run build:sta && cd dist/showsourcing && aws s3 sync . s3://app-sta.showsourcing.com --delete && aws s3 cp s3://app-sta.showsourcing.com/index.html s3://app-sta.showsourcing.com/index.html --metadata-directive REPLACE --cache-control max-age=0",
    "deploy:production": "npm run build && cd dist/showsourcing && aws s3 sync . s3://app.showsourcing.com --delete && aws s3 cp s3://app.showsourcing.com/index.html s3://app.showsourcing.com/index.html --metadata-directive REPLACE --cache-control max-age=0",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en/translations.json ./src/assets/i18n/zh/translations.json ./src/assets/i18n/fr/translations.json ./src/assets/i18n/es/translations.json --clean --sort --format namespaced-json -m _"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.0",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/pwa": "^0.13.1",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "8.2.14",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ng-select/ng-select": "^3.0.7",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angulartics2": "^7.5.2",
    "apollo-angular": "^1.5.0",
    "apollo-angular-link-http": "^1.6.0",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-ws": "^1.0.17",
    "chart.js": "^2.8.0",
    "chartjs-plugin-annotation": "^0.5.7",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.0",
    "getstream": "^4.1.0",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "hubspot": "^2.1.1",
    "ng2-charts": "^2.3.0",
    "npm": "^6.4.1",
    "realm-graphql-client": "^1.4.0",
    "rxjs": "^6.4.0",
    "subscriptions-transport-ws": "^0.9.14",
    "uuid": "^3.2.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.3",
    "@angular/cli": "^8.3.19",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^3.0.4",
    "@compodoc/compodoc": "^1.1.7",
    "@types/file-saver": "^2.0.0",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.9.3",
    "@types/uuid": "^3.4.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "moment": "^2.24.0",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "typescript": "3.4.5",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
