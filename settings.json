{
  "path-intellisense.mappings": {
		"~app/*": ["${workspaceRoot}/src/app/*"],
		"~features/*": ["${workspaceRoot}/src/app/features/*"],
		"~auth/*": ["${workspaceRoot}/src/app/features/auth/*"],
		"~products/*": ["${workspaceRoot}/src/app/features/products/*"],
		"~projects/*": ["${workspaceRoot}/src/app/features/projects/*"],
		"~utils/*": ["${workspaceRoot}/src/app/app-root/utils/*"],
		"~user/*": ["${workspaceRoot}/src/app/features/user/*"],
		"~suppliers/*": ["${workspaceRoot}/src/app/features/suppliers/*"],
		"~comment/*": ["${workspaceRoot}/src/app/features/comment/*"],
		"~root/*": ["${workspaceRoot}/src/app/app-root/*"],
		"~store/*": ["${workspaceRoot}/src/app/app/store/*"],
		"~shared/*": ["${workspaceRoot}/src/app/shared/*"],
		"~tasks/*": ["${workspaceRoot}/src/app/features/tasks/*"],
		"~dialog/*": ["${workspaceRoot}/src/app/features/dialog"],
  },
}
